{
  "name": "segmenthl7tools",
  "version": "1.0.5",
  "description": "Ferramentas para manipulação de segmentos HL7",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "release": "release-it",
    "prepare": "husky install",
    "biome:format": "npx @biomejs/biome format --write ./src",
    "biome:lint": "npx @biomejs/biome lint ./src",
    "biome:check": "npx @biomejs/biome check ./src"
  },
  "keywords": [
    "hl7",
    "segment",
    "healthcare",
    "medical",
    "interoperability",
    "segmenthl7tools"
  ],
  "author": "ICeranto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/igorceranto/segmentHL7Tools"
  },
  "bugs": {
    "url": "https://github.com/igorceranto/segmentHL7Tools/issues"
  },
  "homepage": "https://github.com/igorceranto/segmentHL7Tools#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@types/node": "^20.8.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "husky": "8.0.3",
    "release-it": "^19.0.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  }
}
